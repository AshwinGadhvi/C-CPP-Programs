
#include<stdio.h>
#include<conio.h>

void main()
{
	int i,j,a[10][10],rows,cols;
	clrscr();
	printf ("how many rows::");
	scanf ("%d",&rows);
	printf ("how many cols::");
	scanf ("%d",&cols);
	printf ("Enter two D array /elements \n");
	for(i=0;i<rows;i++)
	{
		for(j=0;j<cols;j++)
		{
			printf ("a[%d][%d]",i,j);
			scanf ("%d",&a[i][j]);
		}
	}
	printf ("\n 2 D array \elements::\n");
	for(i=0;i<rows;i++)
	{
		for(j=0;j<cols;j++)
		{
			printf (" %d ",a[i][j]);
		}
		printf ("\n");
	}
	printf ("transpose array is::\n");
	for(i=0;i<rows;i++)
	{
		for(j=0;j<cols;j++)
		{
			printf (" %d ",a[j][i]);
		}
		printf ("\n");
	}
	getch();
}